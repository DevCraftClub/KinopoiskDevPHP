# Kinopoisk API Configuration for Testing
# Copy this file to .env.testing and set your actual API token
KINOPOISK_API_TOKEN=your_test_api_token_here

# Application Configuration for Testing
APP_ENV=testing
APP_DEBUG=true

# Cache Configuration for Testing
CACHE_DRIVER=array
CACHE_TTL=300

# Session Configuration for Testing
SESSION_DRIVER=array

# Queue Configuration for Testing
QUEUE_DRIVER=sync

# Logging Configuration for Testing
LOG_LEVEL=debug 